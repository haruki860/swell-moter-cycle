{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "DOM",
      "ES6",
      "DOM.Iterable",
      "ScriptHost"
    ],

    "module": "commonjs" ,
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true ,
    "strict": true ,
    "skipLibCheck": true ,
    "jsx": "react-jsx",
  },
  "include": ["src/**/*"],// 追加
    "exclude": ["node_modules"]
}
